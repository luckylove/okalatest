<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

    <settings>
        <setting name="cacheEnabled" value="false"/>
        <setting name="useGeneratedKeys" value="true"/>
        <setting name="defaultExecutorType" value="SIMPLE"/>
        <setting name="jdbcTypeForNull" value="NULL" />
    </settings>

    <typeAliases>
        <!-- Common Code -->
        <typeAlias alias="userModel" type="com.marinabay.cruise.model.User"/>
        <typeAlias alias="userGroupModel" type="com.marinabay.cruise.model.UserGroup"/>
        <typeAlias alias="pagingModel" type="com.marinabay.cruise.model.PagingModel"/>
        <typeAlias alias="cruiseModel" type="com.marinabay.cruise.model.Cruise"/>
        <typeAlias alias="scheduleModel" type="com.marinabay.cruise.model.Schedules"/>
        <typeAlias alias="activeUserModel" type="com.marinabay.cruise.model.ActiveUser"/>
        <typeAlias alias="notificationModel" type="com.marinabay.cruise.model.Notification"/>

    </typeAliases>
</configuration>
